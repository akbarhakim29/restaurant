<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bymankind.restaurant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_resto"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminActivity" />
        <activity android:name=".KokiActivity" />
        <activity android:name=".PramusajiActivity" />
        <activity android:name=".KasirActivity" />
        <activity android:name=".Position.CreatePosition" />
        <activity android:name=".Position.ListPosition" />
        <activity android:name=".Menu.CreateMenu" />
        <activity android:name=".Menu.ListMenu" />
        <activity android:name=".Menu.DetailMenu" />
        <activity
            android:name=".Employee.ListEmployee"
            android:label="@string/title_activity_list_employee"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Employee.DetailEmployee" />
        <activity android:name=".Position.DetailPosition" />
        <activity android:name=".TypesOfMenu.ListTypesofmenu" />
        <activity android:name=".TypesOfMenu.DetailTypesofmenu" />
        <activity android:name=".Table.ListTable" />
